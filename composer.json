{
    "name": "angel-source-labs/laravel-spatial",
    "description": "Spatial data types extension for Laravel.",
    "scripts": {
        "test": "phpunit -c phpunit.xml.dist",
        "test:unit": "phpunit -c phpunit.xml.dist --testsuite unit",
        "test:integration": "phpunit -c phpunit.xml.dist --testsuite integration"
    },
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Brion Finlay"
        }
    ],
    "repositories": [
        {
            "type": "path",
            "url": "../laravel-expressions"
        }
    ],
    "require": {
        "php": "^7.2.5|^8.0",
        "ext-pdo": "*",
        "ext-json": "*",
        "angel-source-labs/laravel-expressions": "dev-master",
        "doctrine/dbal": "^2.6",
        "geo-io/wkb-parser": "^1.0",
        "jmikola/geojson": "^1.0",
        "laravel/framework": "^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "AngelSourceLabs\\LaravelSpatial\\": "src/"
        }
    },
    "autoload-dev" : {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AngelSourceLabs\\LaravelSpatial\\SpatialServiceProvider"
            ]
        }
    }
}
